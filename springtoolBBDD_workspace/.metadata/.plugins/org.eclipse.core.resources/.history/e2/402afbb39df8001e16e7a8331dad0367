package com.salesianostriana.dam.EjemploSpringCore;

import org.springframework.stereotype.Component;

import jakarta.annotation.PostConstruct;

@Component
public class MainDeMentira {

	@PostConstruct
	public void init() {
		
		Saludator s = new SpanishSaludator();
		
		s.imprimirSaludo();
		
	}
	
}
